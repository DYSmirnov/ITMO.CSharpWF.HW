<Window x:Class="Ex4.WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ex4.WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button1" Content="Set Name" HorizontalAlignment="Left" Margin="91,95,0,0" VerticalAlignment="Top" Width="75" Click="Button1_Click"/>
        <Button x:Name="button2" Content="Ret Name" HorizontalAlignment="Left" Margin="91,204,0,0" VerticalAlignment="Top" Width="75" Click="Button2_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="244,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="244,204,0,0" VerticalAlignment="Top" Width="449"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click" />
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="myTextBox" />
    </Canvas>
</Window>
